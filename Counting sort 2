int* countingSort(int arr_count, int* arr, int* result_count) {
     static int c[100]={0},i,j,k=0;
    for(i=0;i<arr_count;i++)
      c[arr[i]]++;
    for(i=0;i<100;i++)
      {
        for(j=0;j<c[i];j++)
        {
            arr[k++]=i;
        }
      }
  *result_count=arr_count;
  return arr;
}
